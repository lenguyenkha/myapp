      <div class="panel-heading">
        <%= link_to t(".add_new"), new_admin_product_path, class: "btn btn-primary" %>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col-lg-12" id="list-product">
    <%= render "list_product" %>
    </div>
  </div>
</div>
